package day1_1;
import java.util.Scanner;
public class Day1_5 {
	
	public static void main(String [] args)
	{  
		int num1,num2;
		int sum;
		
	   Scanner sn=new Scanner(System.in);
	   System.out.println("enter first value:");
	   num1=sn.nextInt();
	   System.out.println("enter a second value");
	   num2=sn.nextInt();
	   sum=num1+num2;
		System.out.println("sum:"+sum);
	}
	

}
